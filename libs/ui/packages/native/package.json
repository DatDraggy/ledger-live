  "license": "MIT",
  "author": "Ledger Live Team <team-live@ledger.fr>",
  "main": "lib/index.js",
  "types": "index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "lib/*",
        "lib/*/index.d.ts",
        "./*"
      ]
    }
  },
  "exports": {
    ".": "./lib/index.js",
    "./lib/*": "./lib/*.js",
    "./lib/*/": "./lib/*/index.js",
    "./*": "./lib/*.js",
    "./*/": "./lib/*/index.js",
    "./": "./lib/",
    "./package.json": "./package.json"
  },
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "android": "expo start --android",
    "build": "tsc -p tsconfig.prod.json && node scripts/postBuild",
    "prebuild:storybook": "pnpm -F ui-shared -F icons-ui -F crypto-icons-ui build",
    "build:storybook": "storybook build -c .storybook-web -o web-build",
    "watch": "tsc -p tsconfig.prod.json --watch",
    "clean": "rimraf lib",
    "eject": "expo eject",
    "ios": "expo start --ios",
    "lint": "eslint src storybook --ext .ts,.tsx,.js,.jsx --cache",
    "lint:fix": "pnpm lint --fix",
    "lint:css": "stylelint './src/**/*.{js,jsx,ts,tsx}'",
    "release": "yarn publish ./lib --access public",
    "expo-start": "expo start",
    "storybook": "storybook dev  -c .storybook-web -p 6006",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "test": "exit 0",
    "web": "expo start --web",
    "unimported": "unimported"
  },
  "dependencies": {
    "@ledgerhq/crypto-icons-ui": "workspace:^",
    "@ledgerhq/icons-ui": "workspace:^",
    "@ledgerhq/ui-shared": "workspace:^",
    "color": "4.2.3",
    "react-native-modal": "13.0.1",
    "rn-range-slider": "2.1.1",
    "styled-system": "5.1.5"
  },
